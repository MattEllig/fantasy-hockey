{
  "name": "server",
  "version": "1.0.0",
  "description": "Tools for fantasy hockey (server)",
  "main": "index.js",
  "engines": {
    "node": "14.15.3",
    "npm": "6.14.10"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon --ignore client/ index.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "npm install --also=dev --prefix client && npm run build --prefix client"
  },
  "author": "Matt Ellig <matt.ellig@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mattellig/fantasy-hockey-tools"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "connect-mongodb-session": "^2.4.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "mongoose": "^5.11.8",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "winston": "^3.3.3",
    "winston-mongodb": "^5.0.5"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6"
  }
}
